{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "react-router build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "node ./server/dev-server.js",
    "dev:caddy": "run-p start:caddy dev",
    "start": "cross-env NODE_ENV=production node index.js",
    "start:caddy": "caddy run --config ./caddy/Caddyfile",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true react-router-serve ./build/server/index.js",
    "setup": "npm run build && playwright install",
    "format": "prettier --write \"./**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"./**/*.{ts,tsx,mdx}\" --cache",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --quiet",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "typegen": "react-router typegen",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.2.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@t3-oss/env-core": "^0.12.0",
    "close-with-grace": "^2.2.0",
    "dotenv": "^16.4.7",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "set-cookie-parser": "^2.7.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.1",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/set-cookie-parser": "^2.4.10",
    "@vitest/browser": "^3.0.7",
    "msw": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.50.1",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7",
    "vitest-browser-react": "^0.1.1"
  }
}
