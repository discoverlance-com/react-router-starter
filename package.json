{
  "name": "react-router-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "react-router build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "node ./server/dev-server.js",
    "dev:caddy": "run-p start:caddy dev",
    "start": "cross-env NODE_ENV=production node index.js",
    "start:caddy": "caddy run --config ./caddy/Caddyfile",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true react-router-serve ./build/server/index.js",
    "setup": "npm run build && playwright install",
    "format": "prettier --write \"./**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"./**/*.{ts,tsx,mdx}\" --cache",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --quiet",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "typegen": "react-router typegen",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@epic-web/cachified": "^5.5.1",
    "@epic-web/client-hints": "^1.3.5",
    "@epic-web/invariant": "^1.0.0",
    "@epic-web/remember": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/express": "^7.3.0",
    "@react-router/fs-routes": "^7.3.0",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "@t3-oss/env-core": "^0.12.0",
    "address": "^2.0.3",
    "class-variance-authority": "^0.7.1",
    "close-with-grace": "^2.2.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "execa": "^9.5.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.3.0",
    "get-port": "^7.1.0",
    "glob": "^11.0.1",
    "helmet": "^8.0.0",
    "isbot": "^5.1.17",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.477.0",
    "morgan": "^1.10.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "remix-utils": "^8.1.0",
    "set-cookie-parser": "^2.7.1",
    "sonner": "^2.0.1",
    "source-map-support": "^0.5.21",
    "spin-delay": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.1",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.3.0",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@types/set-cookie-parser": "^2.4.10",
    "esbuild": "^0.25.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.50.1",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": "20"
  }
}
